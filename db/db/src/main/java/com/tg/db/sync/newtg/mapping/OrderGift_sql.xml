<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.orderGift.OrderGift">
<resultMap type="util.db.sync.newtg.pojo.orderGift.OrderGift" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="gift_id" property="giftId" jdbcType="INTEGER"/>
	<result column="num" property="num" jdbcType="INTEGER"/>
	<result column="cost_price" property="costPrice" jdbcType="DOUBLE"/>
	<result column="price" property="price" jdbcType="DOUBLE"/>
	<result column="msg" property="msg" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="order_id" property="orderId" jdbcType="BIGINT"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.gift_id
	, t.num
	, t.cost_price
	, t.price
	, t.msg
	, t.create_time
	, t.order_id
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from order_gift t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from order_gift t where 
			id=#{value}
		
</select>
</mapper>