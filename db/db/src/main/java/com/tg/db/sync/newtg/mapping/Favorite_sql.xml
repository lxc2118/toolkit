<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.favorite.Favorite">
<resultMap type="util.db.sync.newtg.pojo.favorite.Favorite" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="user_id" property="userId" jdbcType="INTEGER"/>
	<result column="item_id" property="itemId" jdbcType="INTEGER"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="shop_id" property="shopId" jdbcType="INTEGER"/>
	<result column="type" property="type" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.user_id
	, t.item_id
	, t.create_time
	, t.shop_id
	, t.type
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from favorite t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from favorite t where 
			id=#{value}
		
</select>
</mapper>