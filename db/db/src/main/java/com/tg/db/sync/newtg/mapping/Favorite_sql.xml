<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tg.db.sync.newtg.pojo.favorite.Favorite">
<resultMap type="com.tg.db.sync.newtg.pojo.favorite.Favorite" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="user_id" property="userId" jdbcType="INTEGER"/>
	<result column="item_id" property="itemId" jdbcType="INTEGER"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="shop_id" property="shopId" jdbcType="INTEGER"/>
	<result column="type" property="type" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.user_id
	, t.item_id
	, t.create_time
	, t.shop_id
	, t.type
</sql>

<select id="findLimit" resultMap="BaseResultMap" parameterType="com.tg.db.common.Limit" >
	select 
	<include refid="cols" />
	from favorite t limit #{limitStart},#{limitEnd}
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from favorite t where 
			id=#{value}
		
</select>

<insert id="insert" parameterType="com.tg.db.sync.newtg.pojo.favorite.Favorite">
	insert into favorite (
	 id
		, user_id
		, item_id
		, create_time
		, shop_id
		, type
		)values(
	 #{id}
		, #{userId}
		, #{itemId}
		, #{createTime}
		, #{shopId}
		, #{type}
	)
</insert>
</mapper>