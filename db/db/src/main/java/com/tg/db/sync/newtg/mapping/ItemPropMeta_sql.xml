<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tg.db.sync.newtg.pojo.itemPropMeta.ItemPropMeta">
<resultMap type="com.tg.db.sync.newtg.pojo.itemPropMeta.ItemPropMeta" id="BaseResultMap">
	<result column="pid" property="pid" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
	<result column="color_prop" property="colorProp" jdbcType="BIT"/>
	<result column="enum_prop" property="enumProp" jdbcType="BIT"/>
	<result column="sale_prop" property="saleProp" jdbcType="BIT"/>
	<result column="item_prop" property="itemProp" jdbcType="BIT"/>
</resultMap>

<sql id="cols">
	 t.pid
	, t.name
	, t.color_prop
	, t.enum_prop
	, t.sale_prop
	, t.item_prop
</sql>

<select id="findLimit" resultMap="BaseResultMap" parameterType="com.tg.db.common.Limit" >
	select 
	<include refid="cols" />
	from item_prop_meta t limit #{limitStart},#{limitEnd}
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from item_prop_meta t where 
			pid=#{value}
		
</select>

<insert id="insert" parameterType="com.tg.db.sync.newtg.pojo.itemPropMeta.ItemPropMeta">
	insert into item_prop_meta (
	 pid
		, name
		, color_prop
		, enum_prop
		, sale_prop
		, item_prop
		)values(
	 #{pid}
		, #{name}
		, #{colorProp}
		, #{enumProp}
		, #{saleProp}
		, #{itemProp}
	)
</insert>
</mapper>