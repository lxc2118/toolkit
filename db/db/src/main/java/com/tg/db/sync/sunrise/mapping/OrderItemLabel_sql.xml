<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tg.db.sync.sunrise.pojo.orderItemLabel.OrderItemLabel">
<resultMap type="com.tg.db.sync.sunrise.pojo.orderItemLabel.OrderItemLabel" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="supplier_id" property="supplierId" jdbcType="INTEGER"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.name
	, t.create_time
	, t.update_time
	, t.supplier_id
	, t.status
</sql>

<select id="findLimit" resultMap="BaseResultMap" parameterType="com.tg.db.common.Limit" >
	select 
	<include refid="cols" />
	from order_item_label t limit #{limitStart},#{limitEnd}
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from order_item_label t where 
			id=#{value}
		
</select>

<insert id="insert" parameterType="com.tg.db.sync.sunrise.pojo.orderItemLabel.OrderItemLabel">
	insert into order_item_label (
	 id
		, name
		, create_time
		, update_time
		, supplier_id
		, status
		)values(
	 #{id}
		, #{name}
		, #{createTime}
		, #{updateTime}
		, #{supplierId}
		, #{status}
	)
</insert>
</mapper>