<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.alibabaAuth.AlibabaAuth">
<resultMap type="util.db.sync.newtg.pojo.alibabaAuth.AlibabaAuth" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="ali_id" property="aliId" jdbcType="INTEGER"/>
	<result column="resource_owner" property="resourceOwner" jdbcType="VARCHAR"/>
	<result column="member_id" property="memberId" jdbcType="INTEGER"/>
	<result column="expires_in" property="expiresIn" jdbcType="INTEGER"/>
	<result column="refresh_token" property="refreshToken" jdbcType="VARCHAR"/>
	<result column="access_token" property="accessToken" jdbcType="VARCHAR"/>
	<result column="refresh_token_timeout" property="refreshTokenTimeout" jdbcType="TIMESTAMP"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.ali_id
	, t.resource_owner
	, t.member_id
	, t.expires_in
	, t.refresh_token
	, t.access_token
	, t.refresh_token_timeout
	, t.create_time
	, t.update_time
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from alibaba_auth t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from alibaba_auth t where 
			id=#{value}
		
</select>
</mapper>