<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tg.db.sync.sunrise.pojo.itemPropImg.ItemPropImg">
<resultMap type="com.tg.db.sync.sunrise.pojo.itemPropImg.ItemPropImg" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="img_id" property="imgId" jdbcType="INTEGER"/>
	<result column="item_id" property="itemId" jdbcType="INTEGER"/>
	<result column="pid" property="pid" jdbcType="INTEGER"/>
	<result column="vid" property="vid" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.img_id
	, t.item_id
	, t.pid
	, t.vid
</sql>

<select id="findLimit" resultMap="BaseResultMap" parameterType="com.tg.db.common.Limit" >
	select 
	<include refid="cols" />
	from item_prop_img t limit #{limitStart},#{limitEnd}
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from item_prop_img t where 
			id=#{value}
		
</select>

<insert id="insert" parameterType="com.tg.db.sync.sunrise.pojo.itemPropImg.ItemPropImg">
	insert into item_prop_img (
	 id
		, img_id
		, item_id
		, pid
		, vid
		)values(
	 #{id}
		, #{imgId}
		, #{itemId}
		, #{pid}
		, #{vid}
	)
</insert>
</mapper>