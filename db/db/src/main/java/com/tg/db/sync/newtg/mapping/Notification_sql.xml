<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.notification.Notification">
<resultMap type="util.db.sync.newtg.pojo.notification.Notification" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="notification" property="notification" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="supplier_id" property="supplierId" jdbcType="BIGINT"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.notification
	, t.create_time
	, t.supplier_id
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from notification t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from notification t where 
			id=#{value}
		
</select>
</mapper>