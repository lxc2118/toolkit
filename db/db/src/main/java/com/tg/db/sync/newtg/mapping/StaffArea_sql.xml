<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.staffArea.StaffArea">
<resultMap type="util.db.sync.newtg.pojo.staffArea.StaffArea" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="staff_id" property="staffId" jdbcType="INTEGER"/>
	<result column="area_id" property="areaId" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.staff_id
	, t.area_id
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from staff_area t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from staff_area t where 
			id=#{value}
		
</select>
</mapper>