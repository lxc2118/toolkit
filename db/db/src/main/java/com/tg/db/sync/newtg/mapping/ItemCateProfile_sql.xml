<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.itemCateProfile.ItemCateProfile">
<resultMap type="util.db.sync.newtg.pojo.itemCateProfile.ItemCateProfile" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="alias" property="alias" jdbcType="VARCHAR"/>
	<result column="price_ranges" property="priceRanges" jdbcType="VARCHAR"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="ali_cid" property="aliCid" jdbcType="INTEGER"/>
	<result column="ali_cname" property="aliCname" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.alias
	, t.price_ranges
	, t.update_time
	, t.create_time
	, t.ali_cid
	, t.ali_cname
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from item_cate_profile t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from item_cate_profile t where 
			id=#{value}
		
</select>
</mapper>