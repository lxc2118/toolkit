<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.orderTrace.OrderTrace">
<resultMap type="util.db.sync.newtg.pojo.orderTrace.OrderTrace" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="order_id" property="orderId" jdbcType="INTEGER"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="operator_id" property="operatorId" jdbcType="INTEGER"/>
	<result column="operator_type" property="operatorType" jdbcType="VARCHAR"/>
	<result column="type" property="type" jdbcType="VARCHAR"/>
	<result column="description" property="description" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.order_id
	, t.create_time
	, t.operator_id
	, t.operator_type
	, t.type
	, t.description
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from order_trace t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from order_trace t where 
			id=#{value}
		
</select>
</mapper>