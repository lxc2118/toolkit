<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.boothItem.BoothItem">
<resultMap type="util.db.sync.newtg.pojo.boothItem.BoothItem" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="slogan" property="slogan" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="start_time" property="startTime" jdbcType="TIMESTAMP"/>
	<result column="end_time" property="endTime" jdbcType="TIMESTAMP"/>
	<result column="booth_id" property="boothId" jdbcType="INTEGER"/>
	<result column="sort_order" property="sortOrder" jdbcType="INTEGER"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
	<result column="title" property="title" jdbcType="VARCHAR"/>
	<result column="url" property="url" jdbcType="VARCHAR"/>
	<result column="pic_id" property="picId" jdbcType="INTEGER"/>
	<result column="shop_id" property="shopId" jdbcType="INTEGER"/>
	<result column="item_id" property="itemId" jdbcType="INTEGER"/>
	<result column="remark" property="remark" jdbcType="VARCHAR"/>
	<result column="price" property="price" jdbcType="DOUBLE"/>
	<result column="flag" property="flag" jdbcType="VARCHAR"/>
	<result column="flag_color" property="flagColor" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.slogan
	, t.create_time
	, t.update_time
	, t.start_time
	, t.end_time
	, t.booth_id
	, t.sort_order
	, t.status
	, t.title
	, t.url
	, t.pic_id
	, t.shop_id
	, t.item_id
	, t.remark
	, t.price
	, t.flag
	, t.flag_color
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from booth_item t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from booth_item t where 
			id=#{value}
		
</select>
</mapper>