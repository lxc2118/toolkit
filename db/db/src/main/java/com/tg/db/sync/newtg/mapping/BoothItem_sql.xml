<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tg.db.sync.newtg.pojo.boothItem.BoothItem">
<resultMap type="com.tg.db.sync.newtg.pojo.boothItem.BoothItem" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="slogan" property="slogan" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="start_time" property="startTime" jdbcType="TIMESTAMP"/>
	<result column="end_time" property="endTime" jdbcType="TIMESTAMP"/>
	<result column="booth_id" property="boothId" jdbcType="INTEGER"/>
	<result column="sort_order" property="sortOrder" jdbcType="INTEGER"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
	<result column="title" property="title" jdbcType="VARCHAR"/>
	<result column="url" property="url" jdbcType="VARCHAR"/>
	<result column="pic_id" property="picId" jdbcType="INTEGER"/>
	<result column="shop_id" property="shopId" jdbcType="INTEGER"/>
	<result column="item_id" property="itemId" jdbcType="INTEGER"/>
	<result column="remark" property="remark" jdbcType="VARCHAR"/>
	<result column="price" property="price" jdbcType="DOUBLE"/>
	<result column="flag" property="flag" jdbcType="VARCHAR"/>
	<result column="flag_color" property="flagColor" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.slogan
	, t.create_time
	, t.update_time
	, t.start_time
	, t.end_time
	, t.booth_id
	, t.sort_order
	, t.status
	, t.title
	, t.url
	, t.pic_id
	, t.shop_id
	, t.item_id
	, t.remark
	, t.price
	, t.flag
	, t.flag_color
</sql>

<select id="findLimit" resultMap="BaseResultMap" parameterType="com.tg.db.common.Limit" >
	select 
	<include refid="cols" />
	from booth_item t limit #{limitStart},#{limitEnd}
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from booth_item t where 
			id=#{value}
		
</select>

<insert id="insert" parameterType="com.tg.db.sync.newtg.pojo.boothItem.BoothItem">
	insert into booth_item (
	 id
		, slogan
		, create_time
		, update_time
		, start_time
		, end_time
		, booth_id
		, sort_order
		, status
		, title
		, url
		, pic_id
		, shop_id
		, item_id
		, remark
		, price
		, flag
		, flag_color
		)values(
	 #{id}
		, #{slogan}
		, #{createTime}
		, #{updateTime}
		, #{startTime}
		, #{endTime}
		, #{boothId}
		, #{sortOrder}
		, #{status}
		, #{title}
		, #{url}
		, #{picId}
		, #{shopId}
		, #{itemId}
		, #{remark}
		, #{price}
		, #{flag}
		, #{flagColor}
	)
</insert>
</mapper>