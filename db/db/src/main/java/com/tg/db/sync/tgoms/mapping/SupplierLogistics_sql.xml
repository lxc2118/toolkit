<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tg.db.sync.tgoms.pojo.supplierLogistics.SupplierLogistics">
<resultMap type="com.tg.db.sync.tgoms.pojo.supplierLogistics.SupplierLogistics" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="supplier_id" property="supplierId" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
	<result column="code" property="code" jdbcType="VARCHAR"/>
	<result column="cp_id" property="cpId" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.supplier_id
	, t.name
	, t.code
	, t.cp_id
</sql>

<select id="findLimit" resultMap="BaseResultMap" parameterType="com.tg.db.common.Limit" >
	select 
	<include refid="cols" />
	from supplier_logistics t limit #{limitStart},#{limitEnd}
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from supplier_logistics t where 
			id=#{value}
		
</select>

<insert id="insert" parameterType="com.tg.db.sync.tgoms.pojo.supplierLogistics.SupplierLogistics">
	insert into supplier_logistics (
	 id
		, supplier_id
		, name
		, code
		, cp_id
		)values(
	 #{id}
		, #{supplierId}
		, #{name}
		, #{code}
		, #{cpId}
	)
</insert>
</mapper>