<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.staff.Staff">
<resultMap type="util.db.sync.newtg.pojo.staff.Staff" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
	<result column="phone" property="phone" jdbcType="VARCHAR"/>
	<result column="username" property="username" jdbcType="VARCHAR"/>
	<result column="password" property="password" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="site_id" property="siteId" jdbcType="INTEGER"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.name
	, t.phone
	, t.username
	, t.password
	, t.create_time
	, t.update_time
	, t.site_id
	, t.status
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from staff t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from staff t where 
			id=#{value}
		
</select>
</mapper>