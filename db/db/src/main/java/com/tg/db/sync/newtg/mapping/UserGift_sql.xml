<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.userGift.UserGift">
<resultMap type="util.db.sync.newtg.pojo.userGift.UserGift" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="user_id" property="userId" jdbcType="INTEGER"/>
	<result column="gift_id" property="giftId" jdbcType="INTEGER"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="selected" property="selected" jdbcType="BIT"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.user_id
	, t.gift_id
	, t.create_time
	, t.selected
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from user_gift t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from user_gift t where 
			id=#{value}
		
</select>
</mapper>