<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.gift.Gift">
<resultMap type="util.db.sync.newtg.pojo.gift.Gift" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
	<result column="stock_num" property="stockNum" jdbcType="INTEGER"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
	<result column="price" property="price" jdbcType="DOUBLE"/>
	<result column="cost_price" property="costPrice" jdbcType="DOUBLE"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="gift_category_id" property="giftCategoryId" jdbcType="INTEGER"/>
	<result column="pic" property="pic" jdbcType="VARCHAR"/>
	<result column="default_type" property="defaultType" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.name
	, t.stock_num
	, t.status
	, t.price
	, t.cost_price
	, t.create_time
	, t.update_time
	, t.gift_category_id
	, t.pic
	, t.default_type
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from gift t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from gift t where 
			id=#{value}
		
</select>
</mapper>