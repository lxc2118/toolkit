<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.integralRecord.IntegralRecord">
<resultMap type="util.db.sync.newtg.pojo.integralRecord.IntegralRecord" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="user_id" property="userId" jdbcType="INTEGER"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="type" property="type" jdbcType="VARCHAR"/>
	<result column="from_integral" property="fromIntegral" jdbcType="INTEGER"/>
	<result column="remain_integral" property="remainIntegral" jdbcType="INTEGER"/>
	<result column="remark" property="remark" jdbcType="VARCHAR"/>
	<result column="integral" property="integral" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.user_id
	, t.create_time
	, t.type
	, t.from_integral
	, t.remain_integral
	, t.remark
	, t.integral
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from integral_record t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from integral_record t where 
			id=#{value}
		
</select>
</mapper>