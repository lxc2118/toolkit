	<result column="order_id" property="orderId" jdbcType="BIGINT"/>
	<result column="order_item_id" property="orderItemId" jdbcType="BIGINT"/>
	<result column="order_refund_id" property="orderRefundId" jdbcType="BIGINT"/>
	<result column="num" property="num" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.order_id
	, t.order_item_id
	, t.order_refund_id
	, t.num
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
