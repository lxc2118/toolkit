<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.itemCate.ItemCate">
<resultMap type="util.db.sync.newtg.pojo.itemCate.ItemCate" id="BaseResultMap">
	<result column="id" property="id" jdbcType="INTEGER"/>
	<result column="parent_id" property="parentId" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
	<result column="parent" property="parent" jdbcType="BIT"/>
	<result column="sort_order" property="sortOrder" jdbcType="INTEGER"/>
	<result column="props_json" property="propsJson" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="path" property="path" jdbcType="VARCHAR"/>
	<result column="profile_id" property="profileId" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.parent_id
	, t.name
	, t.status
	, t.parent
	, t.sort_order
	, t.props_json
	, t.create_time
	, t.update_time
	, t.path
	, t.profile_id
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from item_cate t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from item_cate t where 
			id=#{value}
		
</select>
</mapper>