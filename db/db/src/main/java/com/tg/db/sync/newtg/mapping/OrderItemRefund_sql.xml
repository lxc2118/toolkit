<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.orderItemRefund.OrderItemRefund">
<resultMap type="util.db.sync.newtg.pojo.orderItemRefund.OrderItemRefund" id="BaseResultMap">
	<result column="id" property="id" jdbcType="BIGINT"/>
	<result column="num" property="num" jdbcType="INTEGER"/>
	<result column="refund_reason" property="refundReason" jdbcType="VARCHAR"/>
	<result column="refund_fee" property="refundFee" jdbcType="DOUBLE"/>
	<result column="refuse_reason" property="refuseReason" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.num
	, t.refund_reason
	, t.refund_fee
	, t.refuse_reason
	, t.create_time
	, t.update_time
	, t.status
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from order_item_refund t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from order_item_refund t where 
			id=#{value}
		
</select>
</mapper>