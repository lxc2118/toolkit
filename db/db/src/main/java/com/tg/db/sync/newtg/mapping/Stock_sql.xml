<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.stock.Stock">
<resultMap type="util.db.sync.newtg.pojo.stock.Stock" id="BaseResultMap">
	<result column="id" property="id" jdbcType="BIGINT"/>
	<result column="order_id" property="orderId" jdbcType="BIGINT"/>
	<result column="matched_id" property="matchedId" jdbcType="BIGINT"/>
	<result column="item_name" property="itemName" jdbcType="VARCHAR"/>
	<result column="num" property="num" jdbcType="INTEGER"/>
	<result column="price" property="price" jdbcType="DOUBLE"/>
	<result column="company_name" property="companyName" jdbcType="VARCHAR"/>
	<result column="company_phone" property="companyPhone" jdbcType="VARCHAR"/>
	<result column="company_address" property="companyAddress" jdbcType="VARCHAR"/>
	<result column="logistics_id" property="logisticsId" jdbcType="VARCHAR"/>
	<result column="sku_names" property="skuNames" jdbcType="VARCHAR"/>
	<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	<result column="update_time" property="updateTime" jdbcType="TIMESTAMP"/>
	<result column="item_pic" property="itemPic" jdbcType="VARCHAR"/>
	<result column="user_name" property="userName" jdbcType="VARCHAR"/>
	<result column="phone" property="phone" jdbcType="VARCHAR"/>
	<result column="status" property="status" jdbcType="VARCHAR"/>
	<result column="door_no" property="doorNo" jdbcType="VARCHAR"/>
	<result column="remark" property="remark" jdbcType="VARCHAR"/>
	<result column="area_id" property="areaId" jdbcType="INTEGER"/>
	<result column="order_item_id" property="orderItemId" jdbcType="INTEGER"/>
</resultMap>

<sql id="cols">
	 t.id
	, t.order_id
	, t.matched_id
	, t.item_name
	, t.num
	, t.price
	, t.company_name
	, t.company_phone
	, t.company_address
	, t.logistics_id
	, t.sku_names
	, t.create_time
	, t.update_time
	, t.item_pic
	, t.user_name
	, t.phone
	, t.status
	, t.door_no
	, t.remark
	, t.area_id
	, t.order_item_id
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from stock t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from stock t where 
			id=#{value}
		
</select>
</mapper>