<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="util.db.sync.newtg.pojo.propValueMeta.PropValueMeta">
<resultMap type="util.db.sync.newtg.pojo.propValueMeta.PropValueMeta" id="BaseResultMap">
	<result column="vid" property="vid" jdbcType="INTEGER"/>
	<result column="name" property="name" jdbcType="VARCHAR"/>
</resultMap>

<sql id="cols">
	 t.vid
	, t.name
</sql>

<select id="findAll" resultMap="BaseResultMap" parameterType="util.db.query.Query" >
	select 
	<include refid="cols" />
	from prop_value_meta t
	<include refid="sql.page"></include>
</select>

<select id="get" resultMap="BaseResultMap" parameterType="Object">
	select <include refid="cols" /> from prop_value_meta t where 
			vid=#{value}
		
</select>
</mapper>