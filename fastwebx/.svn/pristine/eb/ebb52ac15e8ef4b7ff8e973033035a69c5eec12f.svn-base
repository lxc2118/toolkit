package com.fastwebx.test.hero.pojo;
public class Hero{
	private Integer hid;
	private String hname;
	private Integer mid;
	public void setHid(Integer value){
		this.hid=value;
	}
	public Integer getHid(){
		return this.hid;
	}
	public void setHname(String value){
		this.hname=value;
	}
	public String getHname(){
		return this.hname;
	}
	public void setMid(Integer value){
		this.mid=value;
	}
	public Integer getMid(){
		return this.mid;
	}
}
