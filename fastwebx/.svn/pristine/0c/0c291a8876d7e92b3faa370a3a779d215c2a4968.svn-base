package com.fastwebx.common.reflect.valueTrans.imp;

import java.text.DecimalFormat;

import com.fastwebx.common.reflect.valueTrans.ValueTrans;

public class IntTrans implements ValueTrans<Integer>{

	@Override
	public String toStr(Integer val) {
		
		return val+"";
	}

	@Override
	public Integer toObj(String str) {
		
		return Integer.parseInt(str);
	}
	
}
