package huxagon.util.pathWayFinder.imp.tool;

import huxagon.coordinate.Coordinate;
import junit.framework.TestCase;

/**
 * @author mj
 */
public class Rect4PathWayMoveTest extends Rect4PathWaySquareTest {
	private Coordinate build(int x,int y){
		return new Coordinate(x,y);
	}
	public void testMove(){
		System.out.println(Rect4PathWay.move(build(4,0), build(8,0)));
		System.out.println(Rect4PathWay.move(build(4,0), build(0,0)));
		
		System.out.println(Rect4PathWay.move(build(3,3), build(0,0)));
		System.out.println(Rect4PathWay.move(build(3,3), build(2,4)));
	}
}
